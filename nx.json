{
  "npmScope": "speek",
  "affected": { "defaultBase": "main" },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "webapp": { "tags": [] },
    "webapp-e2e": { "tags": [], "implicitDependencies": ["webapp"] },
    "gateway": { "tags": [] },
    "core-entity": { "tags": [] },
    "core-stream": { "tags": [] },
    "ui-components": { "tags": [] },
    "util-format": { "tags": [] },
    "ui-stylesheets": { "tags": [] },
    "core-adapter": { "tags": [] }
  }
}
